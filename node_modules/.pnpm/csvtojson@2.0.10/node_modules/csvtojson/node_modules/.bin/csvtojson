#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jugurthak/epita/equals-backend/node_modules/.pnpm/csvtojson@2.0.10/node_modules/csvtojson/bin/node_modules:/home/jugurthak/epita/equals-backend/node_modules/.pnpm/csvtojson@2.0.10/node_modules/csvtojson/node_modules:/home/jugurthak/epita/equals-backend/node_modules/.pnpm/csvtojson@2.0.10/node_modules:/home/jugurthak/epita/equals-backend/node_modules/.pnpm/node_modules:/home/jugurthak/epita/equals-backend/node_modules:/home/jugurthak/epita/node_modules:/home/jugurthak/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/jugurthak/epita/equals-backend/node_modules/.pnpm/csvtojson@2.0.10/node_modules/csvtojson/bin/node_modules:/home/jugurthak/epita/equals-backend/node_modules/.pnpm/csvtojson@2.0.10/node_modules/csvtojson/node_modules:/home/jugurthak/epita/equals-backend/node_modules/.pnpm/csvtojson@2.0.10/node_modules:/home/jugurthak/epita/equals-backend/node_modules/.pnpm/node_modules:/home/jugurthak/epita/equals-backend/node_modules:/home/jugurthak/epita/node_modules:/home/jugurthak/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/csvtojson" "$@"
else
  exec node  "$basedir/../../bin/csvtojson" "$@"
fi
